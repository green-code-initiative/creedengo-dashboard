{
  "name": "@creedengo/vue-dashboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  },
  "executionEnv": {
    "nodeVersion": "22.17.1"
  },
  "exports": {
    "./script": "./dist/view.js",
    "./stylesheet": "./dist/view.css" 
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "vite build && vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "coverage": "vitest run --coverage",
    "serve:sonar": "node src/api/sonar/mock/server.js",
    "sonar": "pnpm coverage && pnpm exec sonar-scanner -Dsonar.login=$SONAR -Dsonar.host.url=http://localhost:9000",
    "lint": "eslint src --fix",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs"
  },
  "dependencies": {
    "@creedengo/sonar-services": "workspace:*",
    "@creedengo/vue-ui": "workspace: *",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@creedengo/core-services": "workspace: *",
    "@creedengo/sonar-services": "workspace: *",
    "@creedengo/eslint-config": "workspace: *",
    "@creedengo/playwright-config": "workspace:*",
    "@creedengo/vitest-config": "workspace:*",
    "@storybook/addon-a11y": "^10.0.3",
    "@storybook/addon-docs": "^10.0.3",
    "@storybook/addon-links": "^10.0.3",
    "@storybook/builder-vite": "^10.0.3",
    "@storybook/vue3": "^10.0.3",
    "@storybook/vue3-vite": "^10.0.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.6",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.0",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-storybook": "^10.0.3",
    "eslint-plugin-vue": "^10.5.1",
    "jest": "^30.2.0",
    "jsdom": "^27.1.0",
    "msw": "^2.11.6",
    "msw-storybook-addon": "^2.0.6",
    "@playwright/test": "^1.56.1",
    "sonarqube-scanner": "^4.3.2",
    "storybook": "^10.0.3",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
